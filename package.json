    "jschardet": "^3.1.2",
    "link-preview-js": "^3.0.5",
    "mime-types": "^2.1.35",
    "node-cache": "^5.1.2",
    "node-fetch": "^2.7.0",
    "node-xlsx": "^0.24.0",
    "openai": "^5.13.1",

    "amqplib": "^0.10.3",
    "awesome-phonenumber": "^6.2.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jimp": "^0.22.10",
    "jschardet": "^3.0.0",
    "link-preview-js": "^3.0.4",
    "mime-types": "^2.1.35",
    "node-cache": "^5.1.2",
    "node-fetch": "2",
    "node-xlsx": "^0.23.0",
    "pino": "^8.14.1",
    "qrcode": "^1.5.1",
    "qrcode-terminal": "^0.12.0",
    "uuid": "^9.0.0",
    "vcf": "^2.1.1",
    "xlsx": "^0.18.5",
    "yaml": "^2.2.1"
  }
